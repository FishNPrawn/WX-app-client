page {
  background-color: #f5f5f5;
  background-image: linear-gradient(#FFA760, #ffffff 15%, #f5f5f5 20%, #f5f5f5 100%);
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
}
.address .address_btn button {
  color: white;
  border: white 2px solid;
}
.address .revice_address_row {
  width: 95%;
  background-color: white;
  border-radius: 5px;
  margin: auto;
  padding: 13px 0 13px;
}
.address .revice_address_row .user_info {
  margin-left: 10px;
}
.address .revice_address_row .user_info .address {
  display: flex;
  font-size: 15px;
}
.address .revice_address_row .user_info .address image {
  height: 14px;
  width: 7.5px;
  margin-top: 4px;
  position: absolute;
  right: 20px;
}
.address .revice_address_row .name_phone {
  display: flex;
  color: #9e9e9e;
  font-size: 13px;
}
.address .revice_address_row .name_phone .user_phone {
  margin-left: 6px;
}
.box2 {
  width: 95%;
  background-color: white;
  border-radius: 5px;
  margin: auto;
  margin-top: 10px;
  padding: 13px 0 13px;
}
.box2 .cart_main {
  display: flex;
}
.box2 .cart_main .cart_item image {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  margin-left: 10px;
}
.box2 .cart_main .totol_arrow {
  display: flex;
  align-items: center;
  font-size: 13px;
}
.box2 .cart_main .totol_arrow .totol_text {
  color: #737373;
  position: absolute;
  right: 35px;
}
.box2 .cart_main .totol_arrow .right_arrow {
  position: absolute;
  right: 20px;
  height: 14px;
  width: 7.5px;
}
.box2 .cart_main .dot {
  margin-left: 10px;
  margin-top: 12px;
}
.box3 {
  display: flex;
  width: 95%;
  margin: auto;
  margin-top: 5px;
}
.box3 .shunfeng {
  display: flex;
  font-size: 10px;
  margin-left: 10px;
}
.box3 .shunfeng image {
  width: 10px;
  height: 10px;
  margin-top: 2px;
}
.box3 .shunfeng text {
  color: #b3b3b3;
  margin-left: 3px;
}
.box3 .order_today {
  display: flex;
  font-size: 10px;
  position: absolute;
  right: 45%;
}
.box3 .order_today image {
  width: 10px;
  height: 10px;
  margin-top: 2px;
}
.box3 .order_today text {
  color: #b3b3b3;
  margin-left: 3px;
}
.box3 .arrive_tmr {
  display: flex;
  font-size: 10px;
  position: absolute;
  right: 20px;
}
.box3 .arrive_tmr image {
  width: 10px;
  height: 10px;
  margin-top: 2px;
}
.box3 .arrive_tmr text {
  color: #b3b3b3;
  margin-left: 3px;
}
.box4 {
  display: block;
  width: 95%;
  background-color: white;
  border-radius: 5px;
  margin: auto;
  padding: 13px 0 13px;
  margin-top: 5px;
  font-size: 15px;
}
.box4 .box3_inside {
  margin-left: 10px;
}
.box4 .box3_inside .line1 {
  height: 1px;
  width: 97%;
  background-color: #f9f9f9;
  margin-top: 10px;
}
.box4 .box3_inside .totol_price {
  display: flex;
}
.box4 .box3_inside .totol_price .fee_text {
  color: #424242;
}
.box4 .box3_inside .totol_price .fee_number {
  display: flex;
  position: absolute;
  right: 20px;
  color: #343434;
}
.box4 .box3_inside .order_fee {
  display: flex;
  margin-top: 10px;
}
.box4 .box3_inside .order_fee .order_text {
  color: #424242;
}
.box4 .box3_inside .order_fee .order_number {
  display: flex;
  position: absolute;
  right: 20px;
  color: #343434;
}
.box4 .box3_inside .coupon {
  display: flex;
  margin-top: 10px;
}
.box4 .box3_inside .coupon .coupon_text {
  color: #424242;
}
.box4 .box3_inside .coupon .discount_number {
  display: flex;
  position: absolute;
  right: 20px;
  color: #343434;
}
.box5 {
  display: block;
  width: 95%;
  background-color: white;
  border-radius: 5px;
  margin: auto;
  padding: 13px 0 13px;
  margin-top: 10px;
  font-size: 15px;
}
.box5 text {
  color: #424242;
  margin-left: 10px;
}
.box5 .line2 {
  height: 1px;
  background-color: #f9f9f9;
  margin-top: 10px;
}
.box5 .wechat_pay {
  display: flex;
  margin-top: 10px;
  margin-left: 10px;
}
.box5 .wechat_pay .wechat_pay_img {
  height: 20px;
  width: 20px;
}
.box5 .wechat_pay .wechat_pay_text {
  margin-left: 5px;
}
.box5 .wechat_pay .all_chk_wrap {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 20px;
}
.box5 .wechat_pay .all_chk_wrap checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 32rpx;
  height: 32rpx;
  border: 1px solid #9c9c9c;
}
.box5 .wechat_pay .all_chk_wrap checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: 1rpx solid #FFA760;
  background: #FFA760;
}
.box5 .wechat_pay .all_chk_wrap checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
.box6 {
  display: flex;
  width: 95%;
  background-color: white;
  border-radius: 5px;
  margin: auto;
  padding: 13px 0 13px;
  margin-top: 10px;
  font-size: 15px;
}
.box6 text {
  color: #424242;
  margin-left: 10px;
}
.box6 .click_comment {
  position: absolute;
  right: 30px;
  display: flex;
  font-size: 15px;
}
.box6 .click_comment .comment {
  color: #888888;
  width: 200px;
}
.box6 .click_comment image {
  height: 14px;
  width: 7.5px;
  margin-left: 5px;
  margin-top: 3px;
}
.footer_tool {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 140rpx;
  background-color: #fff;
  display: flex;
  border-top: 1rpx solid #f9f9f9;
}
.footer_tool .totol_order_box {
  bottom: 0;
  top: 0;
  margin: auto;
  margin-top: 5px;
}
.footer_tool .totol_order_box .totol_order {
  display: flex;
}
.footer_tool .totol_order_box .totol_order .total_price_wrap {
  display: flex;
  justify-items: center;
  align-items: center;
  text-align: center;
}
.footer_tool .totol_order_box .totol_order .total_price_wrap .total_price_text {
  display: flex;
  position: absolute;
  left: 10px;
}
.footer_tool .totol_order_box .totol_order .total_price_wrap .total_price_text text {
  color: #333333;
  font-size: 12px;
  margin-top: 4px;
}
.footer_tool .totol_order_box .totol_order .total_price_wrap .total_price_text .totol_price_value {
  color: red;
  display: flex;
}
.footer_tool .totol_order_box .totol_order .total_price_wrap .total_price_text .totol_price_value text {
  font-size: 12px;
  color: red;
}
.footer_tool .totol_order_box .totol_order .total_price_wrap .total_price_text .totol_price_value .totol_price_value_1 {
  margin-top: 2px;
  font-size: 16px;
  margin-left: 1px;
}
.footer_tool .totol_order_box .totol_order .pay_button_wrap {
  background-image: linear-gradient(to right, tomato 0%, #F45C43 51%, #e97254 100%);
  margin-left: 85%;
  border-radius: 20px;
  display: flex;
}
.footer_tool .totol_order_box .totol_order .pay_button_wrap .pay_button {
  color: white;
  font-size: 17px;
  font-weight: 400;
  border: none;
  height: 80rpx;
  width: 260rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
}
.detail-page {
  width: 100%;
  height: 100%;
  z-index: 99;
  /* min-height: 320px; */
}
.detail-page .title_close {
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  margin-top: 15px;
}
.detail-page .title_close .totol_good {
  letter-spacing: 1px;
}
.detail-page .title_close .close {
  height: 15px;
  width: 15px;
  color: #cccccc;
  position: absolute;
  right: 15px;
}
.detail-page .line2 {
  height: 1px;
  width: 100%;
  background-color: #f2f2f2;
  margin-top: 10px;
}
.detail-page .cart_content {
  width: 90%;
  display: block;
  width: 95%;
  background-color: white;
  border-radius: 5px;
  margin: auto;
  padding: 15px 0 0;
  margin-top: 15px;
}
.detail-page .cart_content .cart_main .cart_item {
  display: block;
}
.detail-page .cart_content .cart_main .cart_item .cart_item_block {
  display: flex;
  padding-bottom: 13px;
}
.detail-page .cart_content .cart_main .cart_item .cart_item_block image {
  width: 65px;
  height: 65px;
  margin-left: 10px;
}
.detail-page .cart_content .cart_main .cart_item .cart_item_block .cart_info_wrap {
  display: block;
  margin-left: 10px;
}
.detail-page .cart_content .cart_main .cart_item .cart_item_block .cart_info_wrap .wrap_line1 {
  display: flex;
  font-size: 15px;
}
.detail-page .cart_content .cart_main .cart_item .cart_item_block .cart_info_wrap .wrap_line1 .good_price {
  position: absolute;
  right: 25px;
}
.detail-page .cart_content .cart_main .cart_item .cart_item_block .cart_info_wrap .wrap_line3 {
  display: flex;
  font-size: 11px;
  color: #afafaf;
}
.detail-page .cart_content .cart_main .cart_item .line3 {
  height: 0.06rem;
  width: 95%;
  background-color: #f5f5f5;
  margin-bottom: 13px;
  margin-left: 2%;
}
