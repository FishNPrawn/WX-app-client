page {
  background-color: #fff;
  padding: 20rpx;
}
.search_row {
  height: 60rpx;
  display: flex;
}
.search_row .inp {
  background-color: #ebebeb;
  height: 100%;
  padding-left: 20rpx;
  flex: 4;
  border-radius: 20px;
  caret-color: #fcaa68;
}
.search_row .btn {
  width: 110rpx;
  height: 100%;
  font-size: 26rpx;
  font-weight: normal;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
}
.search_content {
  margin-top: 30rpx;
}
.search_content .search_item {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  display: flex;
  align-items: center;
  background-color: #fff;
  border-bottom: 1rpx solid #E0E3DA;
  height: 60rpx;
  padding: 10rpx;
  font-size: 26rpx;
}
.search_content .search_item .search_icon {
  height: 20px;
  width: 20px;
}
