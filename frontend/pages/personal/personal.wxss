page {
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  background: #f6f6f6;
}
.box1 {
  height: 170px;
  width: 100%;
  background: #FFA760;
}
.box1 .box1_row1 {
  display: flex;
  margin-left: 15px;
}
.box1 .box1_row1 .profile_img {
  height: 65px;
  width: 65px;
  border-radius: 50%;
  margin-left: 2%;
  box-shadow: rgba(0, 0, 0, 0.16) 0px 6px 6px;
}
.box1 .box1_row1 .profile_name {
  color: #fff;
  font-size: 20px;
  margin-left: 8px;
  display: flex;
  align-items: center;
}
.box1 .box1_row1 .profile_login {
  padding: 0;
  border: none;
  background: none;
  position: absolute;
  margin-left: 7%;
  margin-top: 10px;
}
.box1 .box1_row2 {
  height: 50px;
  display: flex;
  padding: 25px 35px 5px 35px;
  justify-content: space-between;
}
.box1 .box1_row2 .box1_row2_img_name {
  text-align: center;
}
.box1 .box1_row2 .box1_row2_img_name .box1_row2_icon {
  height: 22px;
  width: 22px;
}
.box1 .box1_row2 .box1_row2_img_name .box1_row2_text {
  font-weight: 300;
  font-size: 12px;
}
.box1 .box1_row3 {
  height: 60px;
  background: white;
  height: 70px;
  width: 94%;
  border-radius: 10px;
  position: absolute;
  left: 0;
  right: 0;
  margin-left: auto;
  margin-right: auto;
}
.box1 .box1_row3 .box1_row3_warper {
  display: flex;
  padding: 15px 30px 15px 30px;
  justify-content: space-between;
}
.box1 .box1_row3 .box1_row3_warper .box1_row3_img_name {
  text-align: center;
}
.box1 .box1_row3 .box1_row3_warper .box1_row3_img_name .box1_row3_icon {
  height: 25px;
  width: 25px;
}
.box1 .box1_row3 .box1_row3_warper .box1_row3_img_name .box1_row3_text {
  font-weight: 300;
  font-size: 12px;
  margin-top: 4px;
}
.box2 {
  display: block;
  width: 94%;
  background-color: white;
  border-radius: 10px;
  margin: auto;
  padding: 13px 0 13px;
  margin-top: 10px;
  font-size: 15px;
  margin-top: 60px;
  box-shadow: rgba(0, 0, 0, 0.08) 0px 4px 12px;
}
.box2 .box2_row_warper {
  display: flex;
  padding: 10px 25px 10px 25px;
  justify-content: space-between;
}
.box2 .box2_row_warper .box2_row_img_name {
  text-align: center;
}
.box2 .box2_row_warper .box2_row_img_name .box2_row_icon {
  height: 28px;
  width: 28px;
}
.box2 .box2_row_warper .box2_row_img_name .box2_row_text {
  font-weight: 300;
  font-size: 12px;
  margin-top: 4px;
}
.guess_like .guess_like_text {
  display: flex;
  text-align: center;
  justify-content: center;
  align-items: center;
  padding: 40px 0 5px 0;
}
.guess_like .guess_like_text .love_img_container {
  margin-top: 5px;
}
.guess_like .guess_like_text .love_img_container .love_img {
  width: 20px;
  height: 20px;
  padding: 0 6px 0 3px;
}
.guess_like .guess_like_text text {
  color: #F45C43;
}
.guess_like .preference_display {
  margin-top: 10px;
  background-color: #f5f5f5;
  border-radius: 30px 30px 0px 0px;
}
.guess_like .preference_display .preference_row {
  display: block;
  margin-top: 10px;
  padding: 0 10px 0 10px;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box {
  height: 15rem;
  width: 93%;
  background-color: #ffffff;
  border-radius: 8px;
  margin: auto;
  box-shadow: rgba(0, 0, 0, 0.04) 0px 3px 5px;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator {
  height: 11rem;
  margin-left: auto;
  margin-right: auto;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_image {
  width: 100%;
  height: 88%;
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_msg {
  margin-left: 6%;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_msg .goods_name {
  font-size: 28rpx;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_msg .goods_sale {
  display: flex;
  font-size: 20rpx;
  color: #4d4d4d;
  font-weight: 300;
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_msg .goods_sale .goods_sale_number {
  font-family: cursive;
  margin-left: 2%;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon {
  display: flex;
  margin-left: 6%;
  margin-top: 26px;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon .goods_price {
  width: 84%;
  color: tomato;
  padding-top: 0.5rem;
  display: flex;
  font-family: cursive;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon .goods_price .dollar_symbol {
  display: inline-block;
  align-self: flex-end;
  font-size: 22rpx;
  font-weight: 300;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon .goods_price .dollar_price_number {
  margin-left: 2px;
  font-size: 30rpx;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon .add_to_cart {
  height: 20px;
  width: 20px;
  margin-top: 8px;
}
.guess_like .ending {
  display: flex;
  text-align: center;
  justify-content: center;
  align-items: center;
  padding: 20px 0 20px 0;
  font-size: 23rpx;
  font-weight: 300;
  color: #999999;
}
.goTop {
  height: 60rpx;
  width: 60rpx;
  position: fixed;
  bottom: 30rpx;
  right: 5%;
  border-radius: 50%;
}
