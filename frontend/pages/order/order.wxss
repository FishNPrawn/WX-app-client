/* 顶部菜单切换 */
page {
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
  background-color: #f2f2f2;
}
.navbar {
  display: flex;
  /* 默认选中菜单 */
  /* 选中菜单样式 */
  /* 字体 */
}
.navbar .item {
  position: relative;
  flex: auto;
  font-size: 27rpx;
  width: 100rpx;
  text-align: center;
  line-height: 80rpx;
  color: #333;
}
.navbar .item.active {
  font-size: 30rpx;
  font-weight: 500;
  color: #F45C43;
  border-bottom: 6rpx solid #F45C43;
}
.navbar .item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
}
.no_item {
  display: block;
  text-align: center;
}
.no_item .empty_cart_container {
  padding: 20px 0 10px 0;
}
.no_item .empty_cart_container .empty_cart {
  width: 40%;
}
.no_item .no_item_text1 {
  color: #6d6d6d;
  font-size: 32rpx;
}
.no_item .no_item_text2 {
  color: #bebebe;
  font-size: 30rpx;
}
.guess_like .guess_like_text {
  display: flex;
  text-align: center;
  justify-content: center;
  align-items: center;
  padding: 50px 0 5px 0;
}
.guess_like .guess_like_text .love_img_container {
  margin-top: 5px;
}
.guess_like .guess_like_text .love_img_container .love_img {
  width: 20px;
  height: 20px;
  padding: 0 6px 0 3px;
}
.guess_like .guess_like_text text {
  color: #F45C43;
}
.guess_like .preference_display {
  margin-top: 10px;
  background-color: #f5f5f5;
  border-radius: 30px 30px 0px 0px;
}
.guess_like .preference_display .preference_row {
  display: block;
  margin-top: 10px;
  padding: 0 10px 0 10px;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box {
  height: 14rem;
  width: 93%;
  background-color: #ffffff;
  border-radius: 3px;
  margin: auto;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator {
  height: 10rem;
  margin-left: auto;
  margin-right: auto;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_image {
  width: 100%;
  height: 85%;
  border-radius: 7rpx;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_msg {
  margin-left: 6%;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_msg .goods_name {
  margin-top: 3%;
  font-size: 28rpx;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_msg .goods_sale {
  display: flex;
  font-size: 20rpx;
  color: #4d4d4d;
  font-weight: 300;
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .good_navigator .goods_msg .goods_sale .goods_sale_number {
  font-family: cursive;
  margin-left: 2%;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon {
  display: flex;
  margin-left: 6%;
  margin-top: 30px;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon .goods_price {
  width: 84%;
  color: tomato;
  padding-top: 0.5rem;
  display: flex;
  font-family: cursive;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon .goods_price .dollar_symbol {
  display: inline-block;
  align-self: flex-end;
  font-size: 22rpx;
  font-weight: 300;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon .goods_price .dollar_price_number {
  margin-left: 2px;
  font-size: 30rpx;
}
.guess_like .preference_display .preference_row .preference_col .navigator_box .add_icon .add_to_cart {
  height: 20px;
  width: 20px;
  margin-top: 7px;
}
.cart_content .cart_main .cart_item .cart_info_wrap {
  height: 100%;
  width: 94%;
  background-color: white;
  border-radius: 15px;
  margin: auto;
  margin-top: 15px;
}
.cart_content .cart_main .cart_item .cart_info_wrap .good_name {
  padding: 15px 5px 10px 10px;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #000000;
  font-size: 28rpx;
}
.cart_content .cart_main .cart_item .cart_info_wrap .good_info {
  display: flex;
}
.cart_content .cart_main .cart_item .cart_info_wrap .good_info .good_img {
  padding: 5px 0 10px 10px;
}
.cart_content .cart_main .cart_item .cart_info_wrap .good_info .good_img image {
  width: 60px;
  height: 60px;
  border-radius: 5px;
}
.cart_content .cart_main .cart_item .cart_info_wrap .good_info .good_price {
  display: block;
  position: absolute;
  right: 6%;
  justify-content: flex-end;
  margin-top: 10px;
}
.cart_content .cart_main .cart_item .cart_info_wrap .good_info .good_price .good_price_info {
  display: flex;
}
.cart_content .cart_main .cart_item .cart_info_wrap .good_info .good_price .good_price_info .rmb {
  font-size: 25rpx;
}
.cart_content .cart_main .cart_item .cart_info_wrap .good_info .good_price .good_list_nums {
  text-align: right;
  font-size: 25rpx;
  color: #a1a1a1;
}
.cart_content .cart_main .cart_item .cart_info_wrap .comment_btn_wrapper {
  padding-bottom: 10px;
  margin-left: 77%;
}
.cart_content .cart_main .cart_item .cart_info_wrap .comment_btn_wrapper .comment_btn {
  border: 1px solid #FFA760;
  height: 30px;
  width: 80px;
  color: #FFA760;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  font-size: 14px;
  border-radius: 20px;
}
